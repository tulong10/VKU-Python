   "source": [
    "\n",
    "T = float(input(\"Nhập nhiệt độ không khí (°C): \"))\n",
    "V = float(input(\"Nhập tốc độ gió (km/h): \"))\n",
    "\n",
    "WCI = 13.12 + 0.6215 * T - 11.37 * (V ** 0.16) + 0.3965 * T * (V ** 0.16)\n",
    "WCI_rounded = round(WCI)\n",
    "print(\"Chỉ số gió lạnh (WCI):\", WCI_rounded)"
   ]
