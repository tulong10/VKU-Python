    "ch = input (\" Nhap vao mot so:\")\n",
    "d = {\"chu_cai\": 0, \"chu_so\": 0}\n",
    "for i in ch:\n",
    "    if i.isalpha()==True:\n",
    "         d[\"chu_cai\"] +=1\n",
    "    elif i.isdigit() == True:\n",
    "         d[\"chu_so\"] +=1\n",
    "    else :\n",
    "        pass\n",
    "print (\"Chu cai\", d[\"chu_cai\"])\n",
    "print (\"Chu so\", d[\"chu_so\"])\n",
    "    "
