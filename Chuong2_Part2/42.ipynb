   "source": [
    "def factorical (n):\n",
    "    if n ==0 or n == 1:\n",
    "        return 1\n",
    "    return n * factorical (n-1)\n",
    "num = int (input(\"Enter a number:\"))\n",
    "result = factorical(num)\n",
    "print (result)"
   ]
