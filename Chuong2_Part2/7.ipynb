   "source": [
    "num = int(input(\"Enter a number:\"))\n",
    "level = int(input(\"Enter a level:\"))\n",
    "sum = 0\n",
    "temp = num\n",
    "while (temp >0):\n",
    "    digit = temp % 10\n",
    "    sum += digit ** level\n",
    "    temp //=10\n",
    "if (num==sum):\n",
    "    print (num,\" is a Amstrong level \",str(level))\n",
    "else:\n",
    "    print (num,\"is not a Amstrong\")\n",
    "    "
